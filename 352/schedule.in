%{

#include "schedule.tab.h"

%}

%option nounput
%option noinput

NUM [0-9]+

DAY m|tu|w|th|f

COURSE "ece"[0-9]{4}

STUDENT [a-z]+

LBRACKET "["

RBRACKET "]"

COMMA ","

%%

{DAY} {printf("%s is a valid day\n", yytext);
	return DAY;
}

{NUM} {printf("%s is a valid NUM\n", yytext);
	return NUM;
}

{COURSE} {printf("%s is a valid course identifier\n", yytext);
	return COURSE;
}

{STUDENT} {printf("%s is a valid student name\n", yytext);
	return STUDENT;
}

{LBRACKET} {
	return LBRACKET;
}

{RBRACKET} {
	return RBRACKET;
}

{COMMA} {
	return COMMA;
}

.	{printf("I don't know what %s is...\n", yytext);}
